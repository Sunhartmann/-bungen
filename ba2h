DNA = ['TTACCTTAAC', 'GATATCTGTC', 'ACGGCGTTCG', 'CCCTAAAGAG', 'CGTCAGAGGT']
Pattern = 'AAA'
k = len(Pattern)
distance = 0

def HammingDistance(s1, s2):


    if len(s1) != len(s2):
        raise ValueError()

    return sum(ch1 != ch2 for ch1, ch2 in zip(s1, s2))

for index, dna in enumerate(DNA):

    greatest_hamming_distance = 0

    for offset in range(len(dna) - k + 1):

        motif = dna[offset:offset + k]

        hamming_distance = HammingDistance(Pattern, motif)

        if hamming_distance > greatest_hamming_distance:
            greatest_hamming_distance = hamming_distance

    distance += greatest_hamming_distance

print(distance)
